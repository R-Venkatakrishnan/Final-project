@model IEnumerable<project2.Models.Product>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<table class="table" style="background-color:lightgray" cellspacing="20" cellpadding="20" border="3">
    <tr>
        <td><b>@Html.DisplayName("Productid")</b> </td>
        <td><b>@Html.DisplayName("Productname")</b></td>
        <td><b>@Html.DisplayName("price")</b></td>


    </tr>

    
        
        @foreach (var item in Model)
        {
<tr>
    <td>
        @Html.DisplayFor(modelitem => item.Productid)
    </td>
    <td>
        @Html.DisplayFor(modelitem => item.Productname)
    </td>
    <td>
        @Html.DisplayFor(modelitem => item.price
   )
    </td>

</tr>
    }

    </table>